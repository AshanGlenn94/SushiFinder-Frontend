"74"},{"filePath":"153","messages":"154","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":"155","usedDeprecatedRules":"74"},"C:\\Users\\Ashan Gleen\\Desktop\\Algorizin\\Semester 2\\React JS\\ReactJS Class - Algorizin\\FoodApp Project\\combinedApp\\sushi-finder-combined-main\\src\\index.js",[],["156","157"],"C:\\Users\\Ashan Gleen\\Desktop\\Algorizin\\Semester 2\\React JS\\ReactJS Class - Algorizin\\FoodApp Project\\combinedApp\\sushi-finder-combined-main\\src\\App.js",[],"C:\\Users\\Ashan Gleen\\Desktop\\Algorizin\\Semester 2\\React JS\\ReactJS Class - Algorizin\\FoodApp Project\\combinedApp\\sushi-finder-combined-main\\src\\reportWebVitals.js",[],"C:\\Users\\Ashan Gleen\\Desktop\\Algorizin\\Semester 2\\React JS\\ReactJS Class - Algorizin\\FoodApp Project\\combinedApp\\sushi-finder-combined-main\\src\\reducer.js",[],"C:\\Users\\Ashan Gleen\\Desktop\\Algorizin\\Semester 2\\React JS\\ReactJS Class - Algorizin\\FoodApp Project\\combinedApp\\sushi-finder-combined-main\\src\\StateProvider.js",[],"C:\\Users\\Ashan Gleen\\Desktop\\Algorizin\\Semester 2\\React JS\\ReactJS Class - Algorizin\\FoodApp Project\\combinedApp\\sushi-finder-combined-main\\src\\LandingPage.js",[],"C:\\Users\\Ashan Gleen\\Desktop\\Algorizin\\Semester 2\\React JS\\ReactJS Class - Algorizin\\FoodApp Project\\combinedApp\\sushi-finder-combined-main\\src\\Checkout.js",["158"],"import React from \"react\";\nimport \"./Checkout.css\";\nimport Subtotal from \"./Subtotal\";\nimport { useStateValue } from \"./StateProvider\";\nimport CheckoutProduct from \"./CheckoutProduct\";\n\nfunction Checkout() {\n  const [{ basket }, dispatch] = useStateValue();\n\n  return (\n    <div className=\"checkout\">\n      <div className=\"checkout__left\">\n        <img className=\"checkout__ad\" src=\"/images/sushi_co1.jpg\" alt=\"\" />\n        <div>\n          {/* <h3>Hello, {user?.email}</h3> */}\n          <h2 className=\"checkout__title\">Your Order</h2>\n          {basket.map((item) => (\n            <CheckoutProduct\n              id={item.id}\n              title={item.title}\n              image={item.image}\n              price={item.price}\n              rating={item.rating}\n              restaurant={item.restaurant}\n            />\n          ))}\n        </div>\n      </div>\n      <div className=\"checkout__right\">\n        <Subtotal />\n      </div>\n    </div>\n  );\n}\n\nexport default Checkout;\n","C:\\Users\\Ashan Gleen\\Desktop\\Algorizin\\Semester 2\\React JS\\ReactJS Class - Algorizin\\FoodApp Project\\combinedApp\\sushi-finder-combined-main\\src\\Header.js",["159"],"import React from \"react\";\nimport \"./Header.css\";\nimport ShoppingBasketIcon from \"@material-ui/icons/ShoppingBasket\";\nimport { Link } from \"react-router-dom\";\nimport { useStateValue } from \"./StateProvider\";\nimport sushiLogo from \"./images/sushiLogo.png\";\n\nfunction Header() {\n  const [{ basket }, dispatch] = useStateValue();\n\n  return (\n    <div className=\"header\">\n      <Link to=\"/\">\n        <img className=\"header__logo\" src={sushiLogo} alt=\"\" />\n      </Link>\n\n      <div className=\"header__nav\">\n        <div className=\"header__option\">\n          <span className=\"header__optionLineOne\">About</span>\n          <span className=\"header__optionLineTwo\">Sushi Finder</span>\n        </div>\n        <div className=\"header__option\">\n          <span className=\"header__optionLineOne\">Health Benifits</span>\n          <span className=\"header__optionLineTwo\">of Sushi</span>\n        </div>\n        <Link to=\"/login\" style={{ textDecoration: \"none\" }}>\n          <div className=\"header__option\">\n            <span className=\"header__optionLineOne\">Hello Guest</span>\n            <span className=\"header__optionLineTwo\">Sign In</span>\n          </div>\n        </Link>\n        <Link to=\"/checkout\">\n          <div className=\"header__optionBasket\">\n            <ShoppingBasketIcon />\n\n            <span className=\"header__optionLineTwo header__basketCount\">\n              {basket?.length}\n            </span>\n          </div>\n        </Link>\n      </div>\n    </div>\n  );\n}\n\nexport default Header;\n","C:\\Users\\Ashan Gleen\\Desktop\\Algorizin\\Semester 2\\React JS\\ReactJS Class - Algorizin\\FoodApp Project\\combinedApp\\sushi-finder-combined-main\\src\\Home.js",["160"],"import React, { useState, useEffect } from \"react\";\nimport \"./Home.css\";\nimport Product from \"./Product\";\nimport RatingRender from \"./RatingRender\";\nimport Ratings from \"./Ratings\";\n\nfunction Home() {\n  const [restaurants, setRestaurants] = useState([]);\n\n  const renderRestaurants = useEffect(() => {\n    fetch(\"http://localhost:5000/api/sushibars\")\n      .then((res) => res.json())\n      .then((response) => {\n        setRestaurants(response);\n      })\n      .catch((err) => {\n        console.error(err);\n      });\n  }, []);\n  return (\n    <div className=\"mainContainer\">\n      <div className=\"hometop\">\n        {restaurants.map(({ title, _id, price, restaurant, rating, image }) => (\n          <Product\n            key={_id}\n            id={_id}\n            title={title}\n            price={price}\n            image={image}\n            restaurant={restaurant}\n            rating={rating}\n          />\n        ))}\n      </div>\n      <div className=\"customerRating\">\n        <div className=\"homeleft\">\n          <Ratings />\n        </div>\n        <div className=\"homeright\">\n          <RatingRender />\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default Home;\n","C:\\Users\\Ashan Gleen\\Desktop\\Algorizin\\Semester 2\\React JS\\ReactJS Class - Algorizin\\FoodApp Project\\combinedApp\\sushi-finder-combined-main\\src\\Login.js",[],"C:\\Users\\Ashan Gleen\\Desktop\\Algorizin\\Semester 2\\React JS\\ReactJS Class - Algorizin\\FoodApp Project\\combinedApp\\sushi-finder-combined-main\\src\\Product.js",["161"],"import React from \"react\";\nimport \"./Product.css\";\nimport { useStateValue } from \"./StateProvider\";\nimport { MdStar } from \"react-icons/md\";\n\nfunction Product({ id, title, image, price, rating, restaurant }) {\n  const [{ basket }, dispatch] = useStateValue();\n  const addToBasket = () => {\n    //dispatch the item to the data layer\n    dispatch({\n      type: \"ADD_TO_BASKET\",\n      item: {\n        id: id,\n        title: title,\n        image: image,\n        price: price,\n        rating: rating,\n        restaurant: restaurant,\n      },\n    });\n  };\n\n  return (\n    <>\n      <div className=\"product\">\n        <img src={image} alt=\"\" />\n        <div className=\"product__info\">\n          <p>Description : {title}</p>\n          <p>Sushibar : {restaurant}</p>\n\n          <p className=\"product__price\">\n            Price : <small>$</small>\n            <strong>{price}</strong>\n          </p>\n          <div className=\"product__rating\">\n            {Array(rating)\n              .fill()\n              .map((_, i) => (\n                <p key={Math.random() * 1000000}>\n                  <MdStar />\n                </p>\n              ))}\n          </div>\n        </div>\n        <button onClick={addToBasket}>Add to Cart</button>\n      </div>\n    </>\n  );\n}\n\nexport default Product;\n","C:\\Users\\Ashan Gleen\\Desktop\\Algorizin\\Semester 2\\React JS\\ReactJS Class - Algorizin\\FoodApp Project\\combinedApp\\sushi-finder-combined-main\\src\\Subtotal.js",["162","163"],"import React, { useState } from \"react\";\nimport \"./Subtotal.css\";\nimport CurrencyFormat from \"react-currency-format\";\nimport { useStateValue } from \"./StateProvider\";\nimport { getBasketTotal } from \"./reducer\";\n\nfunction Subtotal() {\n  const [{ basket }, dispatch] = useStateValue();\n  const [isClicked, setIsClicked] = useState(false);\n\n  const clickedHandler = (e) => {\n    e.preventDefault();\n    alert(\"Order Completed!\");\n  };\n\n  const handleCheckout = () => {\n    if (basket == 0) {\n      alert(\"Cart is empty, please add items to checkout!\");\n    } else {\n      setIsClicked(!isClicked);\n    }\n  };\n  return (\n    <>\n      <div className=\"subtotal\">\n        <CurrencyFormat\n          renderText={(value) => (\n            <>\n              <p>\n                Subtotal ({basket.length} items): <strong>{value}</strong>\n              </p>\n            </>\n          )}\n          decimalScale={2}\n          value={getBasketTotal(basket)}\n          displayType={\"text\"}\n          thousandSeparator={true}\n          prefix={\"$\"}\n        />\n\n        <button onClick={handleCheckout}>Proceed to Checkout</button>\n      </div>\n      {isClicked ? (\n        <div className=\"form\">\n          <div className=\"form_container\">\n            <h1>Please Confirm the order!</h1>\n            {/* <div className=\"form\"> */}\n\n            <div>\n              <h3>You ordered total of {basket.length} items</h3>\n              <h5>\n                <CurrencyFormat\n                  renderText={(value) => (\n                    <>\n                      <h3>\n                        Final Total :<strong> {value}</strong>\n                      </h3>\n                    </>\n                  )}\n                  decimalScale={2}\n                  value={getBasketTotal(basket)}\n                  displayType={\"text\"}\n                  thousandSeparator={true}\n                  prefix={\"$\"}\n                />\n              </h5>\n              <form>\n                <h3>Please enter payment details below</h3>\n                <label htmlFor=\"CardholderName\">Cardholder Name : </label>\n                <input type=\"text\" placeholder=\"cardholder name\" />\n                <label htmlFor=\"CardNumber\">Credit/Debit card Number : </label>\n                <input type=\"number\" placeholder=\"credit/debit card number\" />\n                <label htmlFor=\"CardExp\">Exp Date : </label>\n                <input type=\